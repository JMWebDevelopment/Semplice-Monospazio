{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "sidebar",
			"title": "Sidebar",
			"area": "sidebar"
		}
	],
	"customTemplates": [
		
	],
	"settings": {
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"gradients": [
					{
					"name": "Black to Primary",
					"slug": "black-primary",
					"gradient": "linear-gradient(135deg,rgb(0,0,0) 50%,rgb(153,0,153) 100%)"
				},
				{
					"name": "Black to Secondary",
					"slug": "black-secondary",
					"gradient": "linear-gradient(135deg,rgb(0,0,0) 50%,rgb(0,153,255) 100%)"
				},
				{
					"name": "Black to Tertiary",
					"slug": "black-tertiary",
					"gradient": "linear-gradient(135deg,rgb(0,0,0) 50%,rgb(0,204,204) 100%)"
				}
			],
			"link": true,
			"palette": [
				{
					"name": "Black",
					"slug": "black",
					"color": "#000"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#262626"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#0099ff"
				},
				{
					"name": "Off Black",
					"slug": "off-black",
					"color": "#262626"
				},
				{
					"name": "Off White",
					"slug": "off-white",
					"color": "#efefef"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#fff"
				},
				{
					"name": "Dark Red",
					"slug": "dark-red",
					"color": "#490505"
				},
				{
					"name": "Light Red",
					"slug": "light-red",
					"color": "#F24236"
				},
				{
					"name": "Dark Orange",
					"slug": "dark-orange",
					"color": "#B55724"
				},
				{
					"name": "Light Orange",
					"slug": "light-orange",
					"color": "#F29563"
				},
				{
					"name": "Dark Yellow",
					"slug": "dark-yellow",
					"color": "#9B8816"
				},
				{
					"name": "Light Yellow",
					"slug": "light-yellow",
					"color": "#F3FFB6"
				},
				{
					"name": "Dark Green",
					"slug": "dark-green",
					"color": "#115E00"
				},
				{
					"name": "Light Green",
					"slug": "light-green",
					"color": "#6EF74F"
				},
				{
					"name": "Dark Blue",
					"slug": "dark-blue",
					"color": "#0C0230"
				},
				{
					"name": "Light Blue",
					"slug": "light-blue",
					"color": "#547FEE"
				},
				{
					"name": "Light Purple",
					"slug": "light-purple",
					"color": "#C087DE"
				}
			]
		},
		"custom": {
			"fontWeight": {
				"light": 300,
				"regular": 400,
				"bold": 700
			},
			"lineHeight": {
				"one": 1,
				"heading": 1.1,
				"medium": 1.3,
				"body": 1.5
			}
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"blockGap": null,
			"margin": true,
			"padding": true,
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Ubuntu Mono\", monospace",
					"name": "Ubuntu Mono",
					"slug": "ubuntu-mono"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"slug": "small",
					"size": "14px"
				},
				{
					"name": "Regular",
					"slug": "regular",
					"size": "16px"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "18px"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "24px"
				},
				{
					"name": "xLarge",
					"slug": "x-large",
					"size": "28px"
				},
				{
					"name": "35px",
					"slug": "max-30",
					"size": "clamp(30px, 3vw, 35px)"
				},
				{
					"name": "40px",
					"slug": "max-40",
					"size": "clamp(35px, 4vw, 40px)"
				},
				{
					"name": "50px",
					"slug": "max-50",
					"size": "clamp(40px, 5vw, 50px)"
				}
			],
			"lineHeight": true
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"color": "var(--wp--preset--color--black)",
					"radius": "0",
					"style": "solid",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--black)",
					"text": "var(--wp--preset--color--white)"
				},
				"spacing": {
					"padding": {
						"top": "10px",
						"right": "25px",
						"bottom": "10px",
						"left": "25px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--regular)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"core/buttons": {
				"spacing": {
					"margin": {
						"bottom": "0"
					}
				}
			},
			"core/calendar": {
				"spacing": {
					"margin": {
						"bottom": "30px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			},
			"core/code": {
				"border": {
					"color": "var(--wp--preset--color--black)",
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--black)",
					"text": "var(--wp--preset--color--white)"
				},
				"spacing": {
					"margin": {
						"bottom": "30px"
					},
					"padding": {
						"top": "20px",
						"right": "20px",
						"bottom": "20px",
						"left": "20px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu-mono)",
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			},
			"core/column": {
				"spacing": {
					"margin": {
						"bottom": "30px"
					}
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "0"
					}
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "30px",
						"right": "30px",
						"bottom": "30px",
						"left": "30px"
					}
				}
			},
			"core/embed": {
				"spacing": {
					"margin": {
						"bottom": "30px"
					}
				}
			},
			"core/gallery": {
				"spacing": {
					"margin": {
						"bottom": "30px"
					}
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"top": "0px",
						"bottom": "10px"
					}
				},
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"core/image": {
				"spacing": {
					"margin": {
						"bottom": "30px"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu-mono)"
				},
				"spacing": {
					"margin": {
						"top": "0px",
						"bottom": "30px"
					}
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			},
			"core/post-comments": {
				"spacing": {
					"margin": {
						"bottom": "20px"
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": {
						"bottom": "30px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--max-50)"
				}
			},
			"core/preformatted": {
				"border": {
					"color": "var(--wp--preset--color--black)",
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--black)",
					"text": "var(--wp--preset--color--white)"
				},
				"spacing": {
					"margin": {
						"bottom": "30px"
					},
					"padding": {
						"top": "20px",
						"right": "20px",
						"bottom": "20px",
						"left": "20px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu-mono)",
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			},
			"core/pullquote": {
				"border": {
					"width": "0"
				},
				"spacing": {
					"margin": {
						"bottom": "30px"
					},
					"padding": {
						"top": "20px",
						"right": "20px",
						"bottom": "20px",
						"left": "20px"
					}
				}
			},
			"core/quote": {
				"border": {
					"width": "0"
				},
				"spacing": {
					"margin": {
						"bottom": "35px"
					},
					"padding": {
						"top": "40px",
						"right": "40px",
						"bottom": "40px",
						"left": "40px"
					}
				},
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/search": {
				"spacing": {
					"margin": {
						"bottom": "30px"
					}
				}
			},
			"core/separator": {
				"color": {
					"text": "var(--wp--preset--color--black)"
				},
				"spacing": {
					"margin": {
						"top": "30px",
						"bottom": "30px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/site-tagline": {
				"spacing": {
					"margin": {
						"bottom": "20px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			},
			"core/site-title": {
				"spacing": {
					"margin": {
						"bottom": "0"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--one)"
				}
			},
			"core/social-links": {
				"spacing": {
					"margin": {
						"bottom": "0"
					}
				}
			},
			"core/table": {
				"spacing": {
					"margin": {
						"bottom": "30px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu-mono)",
					"fontSize": "var(--wp--preset--font-size--max-50)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu-mono)",
					"fontSize": "var(--wp--preset--font-size--max-40)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu-mono)",
					"fontSize": "var(--wp--preset--font-size--max-30)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu-mono)",
					"fontSize": "var(--wp--preset--font-size--xlarge)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu-mono)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu-mono)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"margin": "0"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ubuntu-mono)",
			"fontSize": "var(--wp--preset--font-size--regular)",
			"fontWeight": "var(--wp--custom--font-weight--regular)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		}
	}
}
